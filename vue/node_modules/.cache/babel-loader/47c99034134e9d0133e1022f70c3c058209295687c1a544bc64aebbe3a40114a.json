 type=\"danger\"\n              @click=\"search\"\n            >\n              搜索\n            </el-button>\n          </div>\n\n          <!-- 用户信息 -->\n          <div class=\"user-info\">\n            <div class=\"user-box\">\n              <span>zandin</span>\n            </div>\n          </div>\n        </div>\n\n        <!-- 中间大div -->\n        <div class=\"main-flex\">\n          <!-- 左侧分类内容 -->\n          <div class=\"category-container\">\n            <div class=\"category-item\" v-for=\"item in typeData\" :key=\"item.id\">\n              <img :src=\"item.img\" alt=\"\" />\n              <div class=\"category-name\">\n                <a href=\"#\" @click=\"navTo('/front/type?id=' + item.id)\">{{ item.name }}</a>\n              </div>\n            </div>\n          </div>\n\n          <!-- 轮播图 -->\n          <div class=\"carousel-container\">\n            <el-carousel height=\"300px\">\n              <el-carousel-item v-for=\"item in carousel_top\" :key=\"item\">\n                <img :src=\"item\" alt=\"\" />\n              </el-carousel-item>\n            </el-carousel>\n\n            <!-- 侧边轮播图 -->\n            <div class=\"side-carousel\">\n              <div class=\"carousel-left\">\n                <el-carousel height=\"200px\">\n                  <el-carousel-item v-for=\"item in carousel_left\" :key=\"item\">\n                    <img :src=\"item\" alt=\"\" />\n                  </el-carousel-item>\n                </el-carousel>\n              </div>\n              <div class=\"carousel-right\">\n                <el-carousel height=\"200px\">\n                  <el-carousel-item v-for=\"item in carousel_right\" :key=\"item\">\n                    <img :src=\"item\" alt=\"\" />\n                  </el-carousel-item>\n                </el-carousel>\n              </div>\n            </div>\n          </div>\n\n          <!-- 用户信息和推荐商品 -->\n          <div class=\"user-recommendation\">\n            <div class=\"user-avatar\">\n              <img :src=